{
  "name": "vulcan",
  "version": "0.0.0",
  "private": "true",
  "scripts": {
    "build": "vite build",
    "commit": "cz",
    "dev": "vite --port 8080 --host --open",
    "lint": "eslint .",
    "prepare": "husky install",
    "preview": "vite preview --host",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*.{json,yml,md}": "prettier --write --cache --ignore-unknown",
    "package.json": "sort-package-json"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "chrome > 40",
    "not ie 11"
  ],
  "prettier": "@pengzhanbo/prettier-config",
  "dependencies": {
    "@vueuse/core": "^9.3.1",
    "axios": "^1.1.3",
    "js-cookie": "^3.0.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.23",
    "vue": "^3.2.41",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@pengzhanbo/eslint-config-vue-ts": "^0.2.10",
    "@pengzhanbo/prettier-config": "^0.2.10",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "^18.11.4",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-legacy": "^2.2.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/test-utils": "^2.2.0",
    "autoprefixer": "^10.4.12",
    "commitizen": "^4.2.5",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "cypress": "^10.10.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.26.0",
    "eslint-plugin-cypress": "^2.12.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "pnpm": "^7.14.0",
    "postcss": "^8.4.18",
    "postcss-px-to-viewport-8-plugin": "^1.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "sort-package-json": "^2.0.0",
    "terser": "^5.15.1",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.11.3",
    "unplugin-vue-components": "^0.22.9",
    "vconsole": "^3.14.7",
    "vite": "^3.1.8",
    "vite-plugin-inspect": "^0.7.5",
    "vite-plugin-mock-dev-server": "^0.1.0",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.0.9"
  },
  "packageManager": "pnpm@7.14.0",
  "engines": {
    "node": ">=16"
  }
}
