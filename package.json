{
  "name": "vulcan",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "commit": "cz",
    "dev": "vite --port 8080 --host --open",
    "lint": "eslint .",
    "prepare": "husky install",
    "preview": "vite preview --host",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*.{json,yml,md}": "prettier --write --cache --ignore-unknown",
    "package.json": "sort-package-json"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "chrome > 40",
    "not ie 11"
  ],
  "prettier": "@pengzhanbo/prettier-config",
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "axios": "^1.3.4",
    "js-cookie": "^3.0.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.32",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.8.2",
    "@pengzhanbo/eslint-config-vue-ts": "^0.3.2",
    "@pengzhanbo/prettier-config": "^0.3.2",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^18.14.1",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-legacy": "^4.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/test-utils": "^2.3.0",
    "autoprefixer": "^10.4.13",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "cypress": "^12.6.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.34.0",
    "eslint-plugin-cypress": "^2.12.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "pnpm": "^7.27.1",
    "postcss": "^8.4.21",
    "postcss-px-to-viewport-8-plugin": "^1.2.0",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "sort-package-json": "^2.4.1",
    "terser": "^5.16.5",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.15.0",
    "unplugin-vue-components": "^0.24.0",
    "vconsole": "^3.15.0",
    "vite": "^4.1.4",
    "vite-plugin-inspect": "^0.7.15",
    "vite-plugin-mock-dev-server": "^0.4.1",
    "vitest": "^0.28.5",
    "vue-tsc": "^1.1.7"
  },
  "packageManager": "pnpm@7.27.0",
  "engines": {
    "node": "^14.18.0 || >=16"
  }
}
