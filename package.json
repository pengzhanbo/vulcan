{
  "name": "vulcan",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check:size": "npx vite-bundle-visualizer",
    "check:type": "vue-tsc --noEmit",
    "commit": "cz",
    "dev": "vite --port 8080 --host --open",
    "lint": "eslint .",
    "prepare": "husky install",
    "preview": "vite preview --host",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*.{json,yml,md}": "prettier --write --cache --ignore-unknown",
    "package.json": "sort-package-json"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "chrome > 40",
    "not ie 11"
  ],
  "prettier": "@pengzhanbo/prettier-config",
  "dependencies": {
    "@unocss/reset": "^0.53.4",
    "@vueuse/core": "^10.2.0",
    "axios": "^1.4.0",
    "js-cookie": "^3.0.5",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.4",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.11.0",
    "@pengzhanbo/eslint-config-vue": "^0.3.15",
    "@pengzhanbo/prettier-config": "^0.3.15",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^20.3.2",
    "@types/nprogress": "^0.2.0",
    "@unocss/eslint-config": "^0.53.4",
    "@vitejs/plugin-legacy": "^4.0.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/test-utils": "^2.4.0",
    "autoprefixer": "^10.4.14",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^3.0.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.16.0",
    "cypress-vite": "^1.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "eruda": "^3.0.0",
    "eslint": "^8.43.0",
    "eslint-plugin-cypress": "^2.13.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "pnpm": "^8.6.5",
    "postcss": "^8.4.24",
    "postcss-px-to-viewport-8-plugin": "^1.2.2",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "sort-package-json": "^2.4.1",
    "terser": "^5.18.1",
    "typescript": "^5.1.3",
    "unocss": "^0.53.4",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vite-bundle-visualizer": "^0.8.0",
    "vite-plugin-inspect": "^0.7.29",
    "vite-plugin-mock-dev-server": "^1.2.1",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.32.2",
    "vue-tsc": "^1.8.2"
  },
  "packageManager": "pnpm@8.6.5",
  "engines": {
    "node": "^14.18.0 || >=16"
  }
}
