{
  "name": "vulcan",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.11.0",
  "engines": {
    "node": "^16.0.0 || ^18.0.0 || >=20"
  },
  "scripts": {
    "build": "vite build",
    "check:size": "npx vite-bundle-visualizer",
    "check:type": "vue-tsc --noEmit",
    "commit": "cz",
    "dev": "vite --port 8080 --host --open",
    "lint": "eslint .",
    "prepare": "husky install",
    "preview": "vite preview --host",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.0",
    "axios": "^1.6.2",
    "js-cookie": "^3.0.5",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.10",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.3.4",
    "@intlify/unplugin-vue-i18n": "^1.5.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.10.4",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-legacy": "^5.2.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/test-utils": "^2.4.3",
    "autoprefixer": "^10.4.16",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^4.1.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.1",
    "cypress-vite": "^1.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "eruda": "^3.0.1",
    "eslint": "^8.55.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-format": "^0.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "pnpm": "^8.11.0",
    "postcss": "^8.4.32",
    "postcss-px-to-viewport-8-plugin": "^1.2.3",
    "sass": "^1.69.5",
    "sort-package-json": "^2.6.0",
    "terser": "^5.25.0",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.2",
    "vite": "^5.0.6",
    "vite-bundle-visualizer": "^0.11.0",
    "vite-plugin-inspect": "^0.8.1",
    "vite-plugin-mock-dev-server": "^1.4.0",
    "vite-plugin-vue-devtools": "1.0.0-rc.5",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^1.0.2",
    "vue-tsc": "^1.8.25"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "chrome > 40",
    "not ie 11"
  ]
}
